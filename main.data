  construct new(accessors, indices) {
    _accessors = accessors
    _numAccessors = accessors.count
    _indices = indices
    _vertexOffset = 0
    _indexOffset = 0
  }

  vertex(vs){
    for(i in 0..._numAccessors){
      _accessors[i][_vertexOffset] = vs[i]
    }
    _vertexOffset = _vertexOffset +1
  }
