  }

  static normalize(v, d) {
    var norm = Vec3.norm(v)

    if (norm == 0.0) {
      d[0] = 0
      d[1] = 0
      d[2] = 0
      return
    }

    Vec3.scale(v, 1.0 / norm, d)
  }

  static scale(v,s,d){
