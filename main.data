  }

  static mulV(a, v, dst){
    dst[0] = a[0] * v
    dst[1] = a[1] * v
    dst[2] = a[2] * v
  }

  static div(a,b,dst){
    dst[0] = a[0] / b[0]
    dst[1] = a[1] / b[1]
    dst[2] = a[2] / b[2]
  }

  static divV(a, v, dst){
    dst[0] = a[0] / v
    dst[1] = a[1] / v
    dst[2] = a[2] / v
  }

  static sub(v1, v2, dst){
    dst[0] = v1[0] - v2[0]
    dst[1] = v1[1] - v2[1]
    dst[2] = v1[2] - v2[2]
  }

  static extract(all, offset, dst){
    dst[0] = all[offset + 0]
