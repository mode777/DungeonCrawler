}class Container {
  
  construct new() {
    _registry = {}
    _factories = {}
  }
  
  register(type){
    _factories[type.toString] = Fn.new { type.new() }
  }

  registerFactory(type, factory){
    _factories[type.toString] = factory
  }

  registerInstance(type, inst){
