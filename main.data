float dither(float v, float m){
  v = m - floor(v*m);
  return clamp(mod(gl_FragCoord.x+m,v), 0.0, 1.0);
}

void main(){

  const float LOG2 = 1.442695;
  float z = gl_FragCoord.z / gl_FragCoord.w;
