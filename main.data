    var a0 = a[0]
    var a1 = a[1]
    var a2 = a[2]
    var b0 = b[0]
    var b1 = b[1]
    var b2 = b[2]
    dest[0] = a1 * b2 - a2 * b1
    dest[1] = a2 * b0 - a0 * b2
    dest[2] = a0 * b1 - a1 * b0
  }

  static crossn(a,b,dest){
